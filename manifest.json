{
    "manifest_version": 3,
    "name": "St4rflyy twitch alert",
    "description": "Notifie lorsque St4rflyy est en live",
    "version": "0.4",
    "action": {
        "default_popup": "index.html",
        "default_icon": {
            "16": "./image/icon_16_16.png",
            "32": "./image/icon_32_32.png",
            "48": "./image/icon_48_48.png",
            "128": "./image/icon_128_128.png"
        }
    },
    "icons": {
        "16": "./image/icon_16_16.png",
        "32": "./image/icon_32_32.png",
        "48": "./image/icon_48_48.png",
        "128": "./image/icon_128_128.png"
    },
    "permissions": ["alarms", "notifications"],
    "background": {
        "service_worker": "./scripts/service_worker.js",
        "type": "module"
    }
}
